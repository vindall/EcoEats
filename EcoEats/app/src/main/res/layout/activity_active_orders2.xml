<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDF6EE">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Active Orders"
        android:textSize="24dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="372dp"
        android:layout_height="211dp"
        app:srcCompat="@drawable/map_mini"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="58dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="373dp"
        android:layout_height="68dp"
        android:background="#FDF6EE"
        app:srcCompat="@drawable/eta_tab"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="269dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="369dp"
        android:layout_height="112dp"
        app:srcCompat="@drawable/food_order_1"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="379dp" />


    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="369dp"
        android:layout_height="112dp"
        app:srcCompat="@drawable/food_order_2"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="491dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order Details"
        android:textStyle="bold"
        android:textSize="24dp"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="347dp" />

    <ImageButton
        android:id="@+id/walkBtn"
        android:layout_width="186dp"
        android:layout_height="48dp"
        android:backgroundTint="#498C6D"
        app:srcCompat="@drawable/chat_icon"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="623dp" />

    <ImageButton
        android:id="@+id/carBtn"
        android:layout_width="186dp"
        android:layout_height="48dp"
        android:backgroundTint="#498C6D"
        app:srcCompat="@drawable/phone_icon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="623dp" />

    <ImageButton
        android:id="@+id/expandBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/arrow_expand_01"
        tools:layout_editor_absoluteX="341dp"
        tools:layout_editor_absoluteY="188dp" />

    <LinearLayout
        android:id="@+id/navbar"
        android:layout_width="match_parent"
        android:layout_height="60sp"
        android:elevation="200dp"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal"
        android:outlineAmbientShadowColor="#000000"
        android:outlineSpotShadowColor="#000000"
        android:paddingLeft="25sp"
        android:paddingRight="25sp"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:id="@+id/home_group"
            android:layout_width="50sp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/home" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Home" />
        </LinearLayout>

        <View
            android:id="@+id/view5"
            android:layout_width="0sp"
            android:layout_height="0sp"
            android:layout_weight="1" />

        <LinearLayout
            android:id="@+id/order_group"
            android:layout_width="50sp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/order" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Orders" />
        </LinearLayout>

        <View
            android:id="@+id/view8"
            android:layout_width="0sp"
            android:layout_height="0sp"
            android:layout_weight="1" />

        <LinearLayout
            android:id="@+id/history_group"
            android:layout_width="50sp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/clock" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="History" />
        </LinearLayout>

        <View
            android:id="@+id/view9"
            android:layout_width="0sp"
            android:layout_height="0sp"
            android:layout_weight="1" />

        <LinearLayout
            android:id="@+id/profile_group"
            android:layout_width="50sp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/user" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Profile" />

        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>